class DefaultFirebaseOptions {
  /// Accessing this will throw to indicate the real generated file is missing.
  static get currentPlatform {
    throw UnsupportedError(
      'No firebase_options.dart found. Run `flutterfire configure` and add the generated file to `lib/`.',
    );
  }
}

